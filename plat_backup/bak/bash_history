#1435491833
print_dot(){   count=$1;   is_oneline=$2;   if [[ $is_oneline -ge 2 ]];then     echo "The 2nd argument is 0,1, indicating oneline and multiline";     exit;   elif [ -z $is_oneline ];then     is_oneline=0;   else     is_oneline=0;   fi;   n=$count;   while [ $n -gt 0 ];do     echo -n ".";     sleep 0.5;     n=$(( $n - 1 ));     if [ $n -eq 0 ];then       seq_b=$( seq -s "\\b" $(( $count + 1 )) | sed -e 's/[0-9]//g' );       seq_s=$( seq -s " " $(( $count + 1 )) | sed -e 's/[0-9]//g' );       if [[ $is_oneline -eq 0 ]];then echo -en "$seq_b${seq_s}$seq_b";       elif [[ $is_oneline -eq 1 ]];then echo;       else echo -en "$seq_b${seq_s}$seq_b";       fi;       n=$count;     fi;     sleep 0.7;   done; }
#1435491850
